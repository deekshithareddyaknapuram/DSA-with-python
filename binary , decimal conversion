def BinaryToNumber(num):
    n=len(num)
    decimal=0
    power=0
    # c=1
    for i in range(n-1,-1,-1):
        decimal+=int(num[i])*(2**power)
        power+=1
    return decimal
num = input()
print(BinaryToNumber(num))

